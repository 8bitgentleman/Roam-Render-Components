                        "string": "Count Children v2 [[roam/templates]]",
                        ":create/user": {
                            ":user/uid": "rTvmoxu9FGSWal8SbCZiywCL2Nm1"
                        },
                        "create-time": 1627488416817,
                        ":edit/user": {
